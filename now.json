{
	"version": 2,
	"name": "ogunb",
	"builds": [{ "src": "package.json", "use": "@now/static-build" }],
	"alias": "www.ogunb.me",
	"routes": [
		{ "src": "^/js/(.*)", "dest": "/js/$1" },
		{ "src": "^/css/(.*)", "dest": "/css/$1" },
		{ "src": "^/img/(.*)", "dest": "/img/$1" },
		{ "src": ".*", "dest": "/index.html" },
		{ "src": "^/favicon.ico", "dest": "/favicon.ico" }
	]
}
